{
  "name": "nuxt-workers",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Easily run code in web workers in Nuxt.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielroe/nuxt-workers.git"
  },
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "web-workers",
    "performance"
  ],
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "simple-git-hooks && pnpm nuxt-module-build build --stub && pnpm nuxt-module-build prepare && nuxi prepare playground",
    "docs:dev": "nuxi dev docs",
    "docs:build": "nuxi generate docs",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepublishOnly": "pnpm lint && pnpm test",
    "release": "bumpp && pnpm publish",
    "test": "vitest run",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "magic-string": "^0.30.10",
    "mlly": "^1.7.1",
    "pathe": "^1.1.2",
    "ufo": "^1.5.4",
    "unplugin": "^1.11.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/module-builder": "0.8.1",
    "@nuxt/schema": "3.12.4",
    "@nuxt/test-utils": "3.13.1",
    "@vitest/coverage-v8": "2.1.1",
    "@vue/test-utils": "2.4.6",
    "bumpp": "9.4.1",
    "eslint": "9.11.1",
    "happy-dom": "15.7.4",
    "lint-staged": "15.2.10",
    "nuxt": "3.12.4",
    "playwright": "1.45.3",
    "rollup": "4.19.1",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.5.4",
    "vite": "5.4.7",
    "vitest": "2.1.1",
    "vitest-environment-nuxt": "1.0.1",
    "vue": "3.4.33"
  },
  "resolutions": {
    "@nuxt/kit": "3.12.4",
    "@nuxt/schema": "3.12.4",
    "nuxt-workers": "link:."
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,mjs,cjs,json,.*rc}": [
      "npx eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.5.0"
}
